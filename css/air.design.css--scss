/* LAYOUTS --- Page layout styles*/
.ad-left, ad-left {
  display: block;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  height: 100%;
  width: 200px;
  margin: 0;
  padding: 0;
  -webkit-transition: all 0.3s ease-out 0s;
  -moz-transition: all 0.3s ease-out 0s;
  -o-transition: all 0.3s ease-out 0s;
  transition: all 0.3s ease-out 0s; }
  .ad-left.line:before, ad-left.line:before {
    position: relative;
    left: 20px;
    width: 500px;
    top: 16px;
    bottom: 18px;
    height: 300px;
    background-color: #CCB6AF;
    z-index: 100; }
  .ad-left.ad-hide, ad-left.ad-hide {
    left: -200px; }
    .ad-left.ad-hide + .ad-right, ad-left.ad-hide + .ad-right, .ad-left.ad-hide + ad-right, ad-left.ad-hide + ad-right {
      left: 0; }

.ad-right, ad-right {
  display: block;
  position: fixed;
  left: 200px;
  right: 0;
  top: 0;
  bottom: 0;
  height: 100%;
  overflow: auto;
  -webkit-transition: all 0.3s ease-out 0s;
  -moz-transition: all 0.3s ease-out 0s;
  -o-transition: all 0.3s ease-out 0s;
  transition: all 0.3s ease-out 0s; }

